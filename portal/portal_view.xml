<?xml version="1.0"?>
<terp>
<data>
	<menuitem name="Portal"  id="menu_portal_root"/>

	<record model="ir.ui.view" id="view_factory_form">
		<field name="name">Portal Factory</field>
		<field name="model">portal.factory</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="Portal Model">
				<field name="name" select="1"/>
				<field name="pmodel_id" select="1"/>
				<field name="menu_name" select="2"/>
				<field name="parentmenu_id" select="2"/>
				<field name="template_action_id"/>
				<field name="view_id" />
				<field name="group_id" />
				<newline/><separator colspan="4"/>
				<field name="state" select="1"/>
				<button name="enable" string="Enable" states="disabled" type="object"/>
				<button name="disable" string="Disable" states="enabled" type="object"/>
			</form>
		</field>
	</record>



	<record model="ir.ui.view" id="view_model_form">
		<field name="name">Portal Model</field>
		<field name="model">portal.model</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="">
				<field name="name" select="1"/>
				<field name="model_id" select="1"/>
				<field name="view_id" select="2"/>
				<field name="rule_id" select="2"/>
				<field name="access_id"/>
			</form>
		</field>
	</record>





	<record model="ir.actions.act_window" id="action_portal_factory">
		<field name="name">Portal Factory</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">portal.factory</field>
		<field name="view_type">form</field>
	</record>

	<menuitem name="Portal/Configuration/Factory" id="menu_portal_factory" action="action_portal_factory"/>



</data>
</terp>
