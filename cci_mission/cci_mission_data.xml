<?xml version="1.0"?>
<terp>
<data>

<!-- Demo Data for Site -->
	<record id="cci_missions_site_liegevervier0" model="cci_missions.site">
		<field eval="&quot;Liege Vervier&quot;" name="name"/>
		<field eval="&quot;ATA&quot;" name="prefix_ATA"/>
		<field eval="&quot;Site de la Chambre de Commerce de Liege - Verviers&quot;" name="official_name_1"/>
	</record>

<!-- Sequences for name on dossier -->
	<record model="ir.sequence.type" id="seq_certificate_cci">
		<field name="name">Certificate Name</field>
		<field name="code">cci_missions.certificate</field>
	</record>
	<record model="ir.sequence.type" id="seq_legalization_cci">
		<field name="name">Legalization Name</field>
		<field name="code">cci_missions.legalization</field>
	</record>

	<record id="ir_sequence_legalizationseq0" model="ir.sequence">
		<field eval="&quot;cci_missions.legalization&quot;" name="code"/>
		<field eval="&quot;Legalization Seq.&quot;" name="name"/>
		<field eval="1" name="number_next"/>
		<field eval="1" name="number_increment"/>
		<field eval="&quot;%(year)s/&quot;" name="prefix"/>
		<field eval="1" name="active"/>
		<field eval="&quot;/LG&quot;" name="suffix"/>
	</record>
	<record id="ir_sequence_certificateseq0" model="ir.sequence">
		<field eval="&quot;cci_missions.certificate&quot;" name="code"/>
		<field eval="&quot;Certificate Seq.&quot;" name="name"/>
		<field eval="1" name="number_next"/>
		<field eval="1" name="number_increment"/>
		<field eval="&quot;%(year)s/&quot;" name="prefix"/>
		<field eval="1" name="active"/>
		<field eval="&quot;/CERT&quot;" name="suffix"/>
	</record>


<!-- Demo Data for Product -->
	<record id="cci_product_product_0" model="product.product">
