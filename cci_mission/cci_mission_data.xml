		<field name="name">Legalization - copy</field>
		<field name="categ_id" ref="product.cat1"/>
	</record>


<!-- Demo Data for Dossier Type -->
	<record id="cci_missions_dossier_type_certificate0" model="cci_missions.dossier_type">
		<field eval="&quot;CO&quot;" name="code"/>
		<field eval="&quot;Certificate&quot;" name="name"/>
		<field name="copy_product_id" ref="cci_product_product_1"/>
		<field name="site_id" ref="cci_missions_site_liegevervier0"/>
		<field name="sequence_id" ref="ir_sequence_certificateseq0"/>
