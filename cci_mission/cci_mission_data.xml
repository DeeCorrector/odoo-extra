		<field name="uom_po_id" ref="product.product_uom_unit"/>
		<field name="name">CO - original</field>
		<field name="categ_id" ref="product.cat1"/>
	</record>
	<record id="cci_product_product_1" model="product.product">
		<field name="list_price">10.0</field>
		<field name="member_price">6.5</field>
		<field name="standard_price">5</field>
		<field name="uom_id" ref="product.product_uom_unit"/>
		<field name="uom_po_id" ref="product.product_uom_unit"/>
		<field name="name">CO - copy</field>
		<field name="categ_id" ref="product.cat1"/>
	</record>
	<record id="cci_product_product_2" model="product.product">
		<field name="list_price">20.0</field>
		<field name="member_price">16.5</field>
		<field name="standard_price">10</field>
		<field name="uom_id" ref="product.product_uom_unit"/>
		<field name="uom_po_id" ref="product.product_uom_unit"/>
		<field name="name">Legalization - original</field>
		<field name="categ_id" ref="product.cat1"/>
	</record>
	<record id="cci_product_product_3" model="product.product">
		<field name="list_price">0.5</field>
		<field name="member_price">0.35</field>
		<field name="standard_price">0.3</field>
		<field name="uom_id" ref="product.product_uom_unit"/>
