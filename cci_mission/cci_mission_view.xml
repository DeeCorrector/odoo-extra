<!-- view for cci_missions_embassy_folder -->
		<record model="ir.ui.view" id="cci_missions_embassy_folder_form">
			<field name="name">cci_missions.embassy_folder.form</field>
			<field name="model">cci_missions.embassy_folder</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="embassy folder">
					<notebook>
					<page string="General">
						<field name="name" select="1"/>
						<field name="section_id"/> <!-- section_id(default=embassy folder), reaondly = true  remain -->
						<group col="6" colspan="3">
						<field name="date" string=" Creation Date"/>
						<field name="date" string="Invoice Date"/>
						<field name="priority"/>
						</group>
						<field name="partner_id" string="Partner to Invoice"/>
						<field name="partner_address_id"/>
						<group col="8" colspan="6">
						<field name="site_id"/>
						<field name="customer_reference"/>
						<field name="member_price"/>
						<field name="destination_id"/>
						</group>
						<field name="embassy_folder_line_ids">
							<tree string="Embassy Foder Lines">
								<field name="name"/>
								<field name="type"/>
								<field name="courier_cost"/>
								<field name="customer_amount"/>
								<field name="tax_rate"/>
							</tree>
						</field>
						<newline/>
						<field name="invoice_note"/>
						<newline/>
						<field name="state"/> <!-- buttons remain-->
					</page>
					<page string="Extra Info">
						<field name="id" />
						<field name="active" />
						<field name="email_cc" />
						<separator colspan="4" string="Dates"/>
						<field name="create_date" />
						<field name="date_closed" />
						<field name="date_action_last" />
						<field name="date_action_next" />
						<separator colspan="4" string="Links"/>
						<field name="link_ids" />
					</page>
					<page string="Notes">
						<field name="internal_note" />
						<newline/>
						<field name="history_line" />
					</page>
					<page string="History">
						<field name="log_ids"/>
					</page>
				</notebook>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="cci_missions_embassy_folder_tree">
			<field name="name">cci_missions.embassy_folder.tree</field>
