<?xml version="1.0" ?>
<terp>
	<data>
	<!--
	=======================================================
	Offer - Category 
	=======================================================
	-->
		<record id="dm_offer_category_consumablegoods0" model="dm.offer.category">
			<field eval="&quot;general&quot;" name="domain"/>
			<field eval="&quot;Consumable Goods&quot;" name="name"/>
		</record>
		<record id="dm_offer_category_shortterm0" model="dm.offer.category">
			<field name="parent_id" ref="dm_offer_category_consumablegoods0"/>
			<field eval="&quot;general&quot;" name="domain"/>
			<field eval="&quot;Short Term&quot;" name="name"/>
		</record>
		<record id="dm_offer_category_longterm0" model="dm.offer.category">
			<field name="parent_id" ref="dm_offer_category_consumablegoods0"/>
			<field eval="&quot;general&quot;" name="domain"/>
			<field eval="&quot;Long Term&quot;" name="name"/>
		</record>
		<record id="dm_offer_category_serviceable0" model="dm.offer.category">
			<field eval="&quot;general&quot;" name="domain"/>
			<field eval="&quot;Serviceable&quot;" name="name"/>
		</record>
		<record id="dm_offer_category_longterm1" model="dm.offer.category">
			<field name="parent_id" ref="dm_offer_category_serviceable0"/>
			<field eval="&quot;general&quot;" name="domain"/>
			<field eval="&quot;Long Term&quot;" name="name"/>
		</record>
		<record id="dm_offer_category_shortterm1" model="dm.offer.category">
			<field name="parent_id" ref="dm_offer_category_serviceable0"/>
			<field eval="&quot;general&quot;" name="domain"/>
			<field eval="&quot;Short Term&quot;" name="name"/>
		</record>
		<record id="dm_offer_category_rates0" model="dm.offer.category">
			<field eval="&quot;purchase&quot;" name="domain"/>
			<field eval="&quot;Rates&quot;" name="name"/>
		</record>
		<record id="dm_offer_category_variable0" model="dm.offer.category">
			<field name="parent_id" ref="dm_offer_category_rates0"/>
			<field eval="&quot;purchase&quot;" name="domain"/>
			<field eval="&quot;Variable&quot;" name="name"/>
		</record>
		<record id="dm_offer_category_fixed0" model="dm.offer.category">
			<field name="parent_id" ref="dm_offer_category_rates0"/>
			<field eval="&quot;purchase&quot;" name="domain"/>
			<field eval="&quot;Fixed&quot;" name="name"/>
		</record>

	<!--
	=======================================================
	Pre-Offers 
	=======================================================
	-->
		<record id="dm_preoffer_proposalforbidding0" model="dm.preoffer">
			<field eval="&quot;bid_furni&quot;" name="code"/>
<!--			<field name="media_id" ref="dm.dm_media_contacts0"/>-->
			<field eval="&quot;Proposal for bidding&quot;" name="name"/>
			<field name="market_id" ref="base.al"/>
			<field eval="time.strftime('%Y-07-01')" name="plannned_delivery_date"/>
			<field eval="&quot;free&quot;" name="state"/>
			<field name="creator_id" ref="base.user_admin"/>
			<field name="copywriter_id" ref="base.res_partner_agrolait"/>
			<field eval="time.strftime('%Y-%m-01')" name="order_date"/>
			<field eval="&quot;new&quot;" name="type"/>
			<field eval="&quot;123456&quot;" name="order_number"/>
			<field eval="time.strftime('%Y-07-01')" name="delivery_date"/>
		</record>
		<record id="dm_preoffer_test0" model="dm.preoffer">
			<field eval="&quot;test&quot;" name="code"/>
<!--			<field name="media_id" ref="dm.dm_media_newspapers0"/>-->
			<field eval="&quot;test&quot;" name="name"/>
			<field name="market_id" ref="base.au"/>
			<field eval="time.strftime('%Y-10-30')" name="plannned_delivery_date"/>
			<field eval="&quot;free&quot;" name="state"/>
			<field name="creator_id" ref="base.user_demo"/>
			<field name="copywriter_id" ref="base.res_partner_2"/>
			<field eval="time.strftime('%Y-%m-15')" name="order_date"/>
			<field eval="&quot;rewrite&quot;" name="type"/>
			<field eval="&quot;333&quot;" name="order_number"/>
			<field eval="time.strftime('%Y-09-01')" name="delivery_date"/>
			<field eval="&quot;Testing to convert preoffer into offer &quot;" name="summary"/>
		</record>
	
	<!--
	=======================================================
	Offers 
	=======================================================
	-->		
		<record id="dm_offer_discountsforeducationalloan0" model="dm.offer">
			<field name="copywriter_id" ref="base.res_partner_2"/>
			<field name="category_ids" eval="[(6,0,[ref('dm.dm_offer_category_serviceable0')])]"/>
			<field eval="1" name="active"/>
			<field eval="&quot;draft&quot;" name="state"/>
			<field eval="&quot;edu&quot;" name="code"/>
<!--			<field eval="&quot;Valid&quot;" name="legal_state"/>-->
<!--			<field eval="time.strftime('%Y-%m-01')" name="date_purchase"/>-->
<!--			<field eval="&quot;2008-08-31&quot;" name="date_validation"/>-->
			<field eval="&quot;1000000&quot;" name="quotation"/>
			<field name="lang_orig" ref="base.lang_en"/>
			<field eval="&quot;Discounts for Educational Loan&quot;" name="name"/>
			<field name="offer_responsible_id" ref="base.user_admin"/>
		</record>
		
		<record id="dm_offer_specialdiscountsforgirlseduloan0" model="dm.offer">
			<field name="copywriter_id" ref="base.res_partner_c2c"/>
			<field name="category_ids" eval="[(6,0,[ref('dm.dm_offer_category_longterm1')])]"/>
			<field eval="1" name="active"/>
			<field eval="&quot;closed&quot;" name="state"/>
			<field eval="&quot;girl_edu&quot;" name="code"/>
<!--			<field eval="&quot;No Bankruptcy&quot;" name="legal_state"/>-->
<!--			<field eval="time.strftime('%Y-%m-15')" name="date_purchase"/>-->
<!--			<field eval="&quot;2008-09-30&quot;" name="date_validation"/>-->
			<field eval="&quot;1500000&quot;" name="quotation"/>
			<field name="lang_orig" ref="base.lang_en"/>
			<field name="offer_origin_id" ref="dm.dm_offer_discountsforeducationalloan0"/>
			<field eval="&quot;Special Discounts for girls - edu loan&quot;" name="name"/>
			<field name="offer_responsible_id" ref="base.user_admin"/>
		</record>

		<record id="dm_offer_biddingoffurniture0" model="dm.offer">
<!--			<field name="purchase_category_ids" ref="dm.dm_offer_category_variable0"/>-->
			<field name="copywriter_id" ref="base.res_partner_9"/>
			<field eval="&quot;It includes each and every furniture required for business purpose... &quot;" name="notes"/>
			<field eval="1" name="active"/>
			<field eval="&quot;open&quot;" name="state"/>
			<field eval="&quot;furni&quot;" name="code"/>
<!--			<field eval="&quot;Valid&quot;" name="legal_state"/>-->
<!--			<field eval="time.strftime('%Y-%m-19')" name="date_purchase"/>-->
<!--			<field eval="&quot;2008-06-19&quot;" name="date_validation"/>-->
			<field eval="&quot;5000000&quot;" name="quotation"/>
			<field name="lang_orig" ref="base.lang_en"/>
			<field eval="&quot;Bidding of furniture&quot;" name="name"/>
			<field name="offer_responsible_id" ref="base.user_demo"/>
		</record>
	</data>
</terp>