<?xml version="1.0"?>
<terp>
<data>
=============================
Auction adjudication
==============================

	<record model="ir.ui.view" id="view_report_auction_adjudication_tree">
		<field name="name">report.auction.adjudication.tree</field>
		<field name="model">report.auction.adjudication</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Customer/Supplier">
				<field name="name"/>
				<field name="adj_total" />
				<field name="state"/>
			</tree>
		</field>
	</record>

	<record model="ir.ui.view" id="view_report_auction_adjudication_graph">
		<field name="name">report.auction.adjudication.graph</field>
		<field name="model">report.auction.adjudication</field>
		<field name="type">graph</field>
		<field name="arch" type="xml">
			<graph string="Object By day" orientation="horizontal" type="bar">
				<field name="name"/>
				<field name="adj_total" operator="+"/>

			</graph>
		</field>

	</record>


	<record model="ir.actions.act_window" id="action_report_auction_adjudication_tree">
		<field name="res_model">report.auction.adjudication</field>
		<field name="view_type">form</field>
		<field name="view_mode">graph,tree</field>
		<!--<field name="domain">[('paid','Paid'),('invoiced','Invoiced')]</field>-->
	</record>
	<menuitem name="Auction Management/Reporting/Manager/Auction Adjudication" action="action_report_auction_adjudication_tree" id="menu_report_auction_adjudication_tree"/>
	<menuitem name="Auction Management/Reporting/Member/Auction Adjudication" action="action_report_auction_adjudication_tree" id="menu_report_auction_adjudication_tree"/>


=============================
Customer/Supplier
==============================

	<record model="ir.ui.view" id="view_per_seller_customer_tree">
		<field name="name">report.per.seller.customer.tree</field>
		<field name="model">report.per.seller.customer</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Customer/Supplier">
				<field name="name" />
				<field name="partner_id"/>

			</tree>
		</field>
	</record>

	<record model="ir.ui.view" id="view_per_seller_customer_graph">
		<field name="name">report.per.seller.customer.graph</field>
		<field name="model">report.per.seller.customer</field>
		<field name="type">graph</field>
		<field name="arch" type="xml">
			<graph string="Customer/Supplier" type="bar">
				<field name="name" />
				<field name="partner_id" />
			</graph>
		</field>

	</record>


	<record model="ir.actions.act_window" id="action_per_seller_customer_tree">
		<field name="res_model">report.per.seller.customer</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,graph</field>
	</record>
	<menuitem name="Auction Management/Reporting/Member/Customer per Seller" action="action_per_seller_customer_tree" id="menu_per_seller_customer_tree"/>
=============================
My Latest 10 Deposit
==============================

	<record model="ir.ui.view" id="view_latest_deposit_tree">
		<field name="name">report.latest.deposit.tree</field>
		<field name="model">report.latest.deposit</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Customer/Supplier">
				<field name="name" />
				<field name="date_dep" />
				<field name="method"/>
				<field name="tax_id"/>
				<field name="info"/>
			</tree>
		</field>
	</record>


	<record model="ir.actions.act_window" id="action_report_latest_doposit_tree">
		<field name="res_model">report.latest.deposit</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree</field>
		<!--<field name="domain">[('user_id','=',uid)]</field>-->
	</record>
	<menuitem name="Auction Management/Reporting/Member/My Latest 10 Deposit" action="action_report_latest_doposit_tree" id="menu_report_latest_doposit_tree"/>


==============================
My Latest 10 Objects
==============================

	<record model="ir.ui.view" id="view_latest_objects_tree">
		<field name="name">report.latest.objects.tree</field>
		<field name="model">report.latest.objects</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Customer/Supplier">
				<field name="bord_vnd_id" />
				<field name="obj_desc" />
				<field name="obj_num"/>
				<field name="obj_ret"/>
				<field name="obj_comm"/>
				<field name="obj_price"/>
			</tree>
		</field>
	</record>
	<record model="ir.actions.act_window" id="action_report_latest_objects_tree">
		<field name="res_model">report.latest.objects</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree</field>
		<!--<field name="domain">[('user_id','=',uid)]</field>-->
	</record>
	<menuitem name="Auction Management/Reporting/Member/My Latest 10 objects" action="action_report_latest_objects_tree" id="menu_report_latest_objects_tree"/>
=============================
Objects per Day
==============================

	<record model="ir.ui.view" id="view_report_auction_object_date_tree">
		<field name="name">report.auction.object.date.tree</field>
		<field name="model">report.auction.object.date</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Customer/Supplier">
				<field name="name"/>
				<field name="date"/>
				<field name="lot_num"/>
				<field name="lot_type"/>
			</tree>
		</field>
	</record>

	<record model="ir.ui.view" id="view_report_auction_object_date_graph">
		<field name="name">report.auction.object.date.graph</field>
		<field name="model">report.auction.object.date</field>
		<field name="type">graph</field>
		<field name="arch" type="xml">
			<graph string="Object By day" orientation="horizontal" type="bar">
				<field name="date"/>
				<field name="lot_num" operator="+"/>
				<field name="name" group="True"/>
			</graph>
		</field>

	</record>


	<record model="ir.actions.act_window" id="action_report_auction_object_date_tree">
		<field name="res_model">report.auction.object.date</field>
		<field name="view_type">form</field>
		<field name="view_mode">graph,tree</field>
		<field name="domain">[('name','&gt;=',time.strftime('%Y-%m-01')), ('state','=','draft')]</field>
	</record>
	<menuitem name="Auction Management/Reporting/Member/Object Per Day" action="action_report_auction_object_date_tree" id="menu_report_auction_object_date_tree"/>
	<menuitem name="Auction Management/Reporting/Manager/Object Per Day" action="action_report_auction_object_date_tree" id="menu_report_auction_object_date_tree1"/>
</data>
</terp>