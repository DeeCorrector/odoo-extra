<?xml version="1.0"?>
<terp>
<data>
	<!--
	=======================================================
	Trademark Views
	=======================================================
	-->
	<record model="ir.ui.view" id="view_trademark_form">
		<field name="name">dm.trademark.form</field>
		<field name="model">dm.trademark</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="Trademarks">
				<notebook>
				<page string="General">
					<field name="name" select="1" colspan="4"/>
					<field name="partner_id" colspan="4"/>				
				</page>
				<page string="Layout">
					<field name="signature" colspan="4" widget="image"/>
					<field name="header" colspan="4" />
					<field name="logo" colspan="4" widget="image"/>
				</page>
				</notebook>
			</form>
		</field>
	</record>

	<record model="ir.actions.act_window" id="action_trademark_form">
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">dm.trademark</field>
		<field name="view_type">form</field>
		<field name="view_id" ref="view_trademark_form"/>
	</record>
	<menuitem name="Direct Marketing/Trademarks/All Trademarks" id="menu_action_trademark_form" action="action_trademark_form"/>
	
</data>
</terp>