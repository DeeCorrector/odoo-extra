<?xml version="1.0"?>
<terp>
	<data><!--delete model="ir.actions.report.xml" search="[('model','like','huissier.')]"/-->

		<report string="Facture salle"
			model="huissier.dossier" 
			name="huissier.invoice.salle" 
			auto="False"
			menu="False"/>
		<report string="Facture vignette"
		model="huissier.vignettes" 
		name="huissier.label.invoices" 
		auto="False" 
		menu="False"
		/>
		<report 
		string="Vignettes" 
		model="huissier.vignettes"
		name="huissier.labels" 
		xml="huissier/report/labels.xml"
		xsl="huissier/report/labels.xsl"
		auto="False"
		id="report_labels"/>
		
		<!-- xsl attribute is needed for the translation system -->
		<report string="PV" model="huissier.dossier" name="huissier.pv" xsl="huissier/report/pv.xsl" auto="False"/>
		<report string="PV" model="huissier.lots" name="huissier.pv.from_lot" xsl="huissier/report/pv.xsl" auto="False" menu="False"/>
	<report 
		string="Attestation" 
		model="huissier.lots" 
		name="huissier.lots.attestation" 
		xml="huissier/report/attestation.xml" 
		xsl="huissier/report/attestation.xsl"
		auto="False"
		id="report_attestation"/>
	<report
		string="Ticket"	
		id="report_ticket" 
		model="huissier.lots" 
		name="huissier.lots.ticket" 
		xml="huissier/report/ticket.xml" 
		xsl="huissier/report/ticket.xsl"
		auto="False"/>
		
		<report string="Listing palais" model="huissier.dossier" name="huissier.palais" auto="False" menu="False"/>
		<report string="Totaux des ventes" model="huissier.dossier" name="huissier.dossier.totals" xml="huissier/report/total.xml" xsl="huissier/report/total.xsl"/>

</data>
</terp>

